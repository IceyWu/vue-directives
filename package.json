{
  "name": "vue-directives-lib",
  "version": "0.0.48",
  "packageManager": "pnpm@7.0.1",
  "description": "Useful custom commandsðŸŽ‰",
  "keywords": [
    "directives",
    "vue",
    "typescript",
    "vue3",
    "vite",
    "ts"
  ],
  "homepage": "https://github.com/IceyWu/vue-directives-lib/tree/main/#readme",
  "license": "MIT",
  "author": "IceyWu",
  "repository": {
    "type": "git",
    "url": "https://github.com/IceyWu/vue-directives-lib"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "volar.d.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "build": "vite build",
    "preview": "vite preview",
    "lib": "vite build && vue-tsc -p tsconfig.build.json",
    "lint": "prettier --write  \"packages/**/*.{ts,tsx}\" \"src/*.{ts,tsx,vue}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "release:patch": "npm version patch",
    "release": "bumpp --commit --push --tag && npm publish --registry https://registry.npmjs.org",
    "publish": "pnpm lib && git add . && git commit -m \"chore: release\" && git push && npm publish --registry https://registry.npmjs.org"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.7.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.7",
    "@iceywu/utils": "^0.0.42",
    "@vueuse/core": "^10.11.0",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@esbuild-kit/cjs-loader": "^2.4.4",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^9.4.1",
    "compare-func": "^2.0.0",
    "conventional-changelog": "^6.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-custom-config": "^0.3.1",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^4.18.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.77.8",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^2.0.2",
    "vue-i18n": "^9.13.1",
    "vue-tsc": "^2.0.26"
  }
}
